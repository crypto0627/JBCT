{
  "name": "backend",
  "version": "1.0.0",
  "description": "JBCT Private Blockchain Creator",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "pnpm run typeorm migration:generate -- -d ./src/config/postgres.ts src/migration/AutoGeneratedMigration",
    "migration:run": "pnpm run typeorm migration:run -- -d ./src/config/postgres.ts",
    "migration:revert": "pnpm run typeorm migration:revert -- -d ./src/config/postgres.ts"
  },
  "keywords": [
    "AI",
    "Blockchain",
    "JBCT"
  ],
  "author": "JakeKuo",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.17",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "npm": "^11.3.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-oauth2": "^1.4.17",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.0",
    "typeorm": "^0.3.23",
    "uuid": "^11.1.0"
  }
}
